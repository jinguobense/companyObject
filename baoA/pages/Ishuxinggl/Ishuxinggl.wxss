.top{
  height:83rpx;
  width: 100%;
  background-color: #efefef;
  position: fixed;
  top: 0rpx;
  left: 0rpx;
  z-index: 9999;
  display: flex;
  align-items: center;
}
.top>view{
  width: 33.3%;
  height: 83rpx;
  line-height: 83rpx;
  font-size: 29rpx;
  color: #333333;
  text-align: center;
  position: relative;
}
.cont{
  width: 100%;
  height: 100%;
  /* background: #efefef; */
  margin-top: 83rpx;
 box-sizing: border-box;
 padding-bottom: 100rpx;
}
.cont .list{
  height:100rpx;
  width: 100%;
  background: #fff;
  display: flex;
}
.cont .list>view{
  width: 33.3%;
  height: 100rpx;
  line-height: 100rpx;
  font-size: 29rpx;
  color: #333333;
  text-align: center;
  border-bottom: 1rpx solid #f2f2f2;
}
.btn{
  height: 88rpx;
  width: 100%;
  background-color: #ff1b1b;
  text-align: center;
  line-height: 88rpx;
  position: fixed;
  bottom: 0rpx;
  color: #fff;
  z-index: 9999;
}
