.top{
  height:83rpx;
  width: 100%;
  background-color: #efefef;
  position: fixed;
  top: 0rpx;
  left: 0rpx;
  z-index: 9999;
  display: flex;
  align-items: center;
}
page{
  background-color: #fff;
}
.top>view{
  width: 33.3%;
  height: 83rpx;
  line-height: 83rpx;
  font-size: 29rpx;
  color: #333333;
  text-align: center;
  position: relative;
}
.cont{
  width: 100%;
  height: 100%;
  margin-top: 83rpx;
  box-sizing: border-box;
  padding-bottom: 100rpx;
}
.cont .list{
  height:188rpx;
  width: 100%;
  background: #fff;
  display: flex;
}
.cont .list>view{
  width: 33.3%;
  height: 188rpx;
  line-height: 188rpx;
  font-size: 29rpx;
  color: #333333;
  text-align: center;
  border-bottom: 1rpx solid #f2f2f2;
}
.cont .list>view image{
  width: 266rpx;
  height: 133rpx;
  margin-top: 28rpx;
}
.btn{
  height: 88rpx;
  width: 100%;
  background-color: #ff1b1b;
  text-align: center;
  line-height: 88rpx;
  position: fixed;
  bottom: 0rpx;
  color: #fff;
  z-index: 9999;
  margin-top: 300rpx;
}