	display: flex;
	align-items: center;
}
.color-6{
	color:#666;
}
.fs-28 {
	font-size: 28rpx;
}
.btn{
	width: 550rpx;
	height: 90rpx;
	background-color: #ff4040;
	border-radius: 14rpx;
	line-height: 90rpx;
