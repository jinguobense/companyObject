/* pages/mine/setting/setting.wxss */
.setList{
	width:100%;
}
.padding-x-40{
	padding:0 40rpx;
	box-sizing: border-box;
}
.padding-40{
	padding:40rpx;
	box-sizing: border-box;
}
.flex-between {
	display: flex;
	justify-content: space-between;
	align-items: center;
}
.fs-32 {
	font-size: 32rpx;
}
.icon{
	width:36rpx;
	height:32rpx;
}
.setItem{
	width:100%;
	height:120rpx;
	border-bottom: 2rpx solid #f7f7f7;
}
.rightImg{
	width:20rpx;
}
.margin-left-20{
	margin-left:20rpx;
}
.flex {
	display: flex;
	align-items: center;
}
.color-6{
	color:#666;
}
.fs-28 {
	font-size: 28rpx;
}
/* 弹框 */
.model{
	width:100%;
	height:100vh;
	background:rgba(0,0,0,0.6);
	position: fixed;
	top:0;
	left:0;
	z-index:9999;
}
.modelBox{
	width:100%;
	height:100vh;
	display: flex;
	flex-direction: column;
	align-items: center;
	justify-content: center;
}
.modelCon{
	width:600rpx;
	height:180rpx;
	background:#fff;
}
.flex-end{
	display: flex;
	justify-content: flex-end;
	align-items: center;
}
.fs-24{
	font-size: 24rpx;
}
.margin-top-30{
	margin-top:30rpx;
}
.margin-left-50{
	margin-left:50rpx;
}

.color-e4{
	color:#e4393c;
}