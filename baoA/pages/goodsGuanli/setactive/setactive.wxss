
page{
  background: #fff;
}
.cont{
  width: 100%;
  height: 100%;
}
.cont .item{
  height: 110rpx;
  width: 100%;
  box-sizing: border-box;
  padding-left: 42rpx;
  border-bottom: 1rpx solid #efefef;
  font-size: 31rpx;
  color: #333333;
  display: flex;
  align-items: center;
}
.cont .item image{
  width: 44rpx;
  height: 40rpx;
  margin-right: 22rpx;
}
.cont .item:nth-child(2) image{
  width: 44rpx;
	height: 30rpx;
}
