/* rootA/pages/system/system.wxss */
.top{
	width:100%;
	height:144rpx;
	padding:0 50rpx;
	box-sizing: border-box;
	/* box-shadow: 0rpx 1rpx 4rpx 0rpx rgba(0, 0, 0, 0.12); */
	box-shadow:inset 0rpx 1rpx 4rpx 0rpx rgba(0, 0, 0, 0.12);
}
.color-9{
	color: #999;
}
.fs-32{
	font-size: 32rpx;
}
.fs-26{
	font-size: 26rpx;
}
.flex-between{
	display: flex;
	justify-content: space-between;
	align-items: center;
}
.line{
	width:100%;
	height:6rpx;
	display: block;
}
.content{
	padding:0 60rpx;
	box-sizing: border-box;
	margin-top:50rpx;
}
.content text{
	font-family: SourceHanSansCN-Regular;
	font-size: 32rpx;
	font-weight: normal;
	font-stretch: normal;
	line-height: 61rpx;
	letter-spacing: 4rpx;
	color: #333333;
}
